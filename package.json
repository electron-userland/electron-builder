    "globby": "^10.0.1",
    "jest-cli": "^24.8.0",
    "jest-junit": "^6.4.0",
    "jsdoc-to-markdown": "^5.0.0",
    "path-sort": "^0.1.0",
    "sumchecker": "^3.0.0",
    "ts-babel": "6.1.7",
    "ts-jsdoc": "^3.1.1",
