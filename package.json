    "fs-extra": "^8.1.0",
    "hosted-git-info": "^2.7.1",
    "iconv-lite": "^0.5.0",
    "ini": "^1.3.5",
    "is-ci": "^2.0.0",
    "isbinaryfile": "^4.0.1",
    "js-yaml": "^3.13.1",
    "lazy-val": "^1.0.4",
    "lodash.isequal": "^4.5.0",
