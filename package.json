  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/out"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/test/babel-jest.js"
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "[\\/]{1}helpers[\\/]{1}"
    ],
    "testRegex": "\\.js$",
    "modulePaths": [
      "<rootDir>/packages"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jestSetup.js"
    ]
  }
}
