{
  "extends": "../tsconfig-base.json",
  "compilerOptions": {
    "rootDir": "src",
    "module": "commonjs",
    "outDir": "out",
    "skipLibCheck": true,
    "baseUrl": "../packages",
    "esModuleInterop": true
  },
  "declaration": false,
  "include": [
    "../typings/*.d.ts",
    "typings/*.d.ts",
    "src/**/*.ts"
  ],
  "exclude": [
    "../typings/electron.d.ts"
  ]
}
